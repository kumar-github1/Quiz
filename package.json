{
    "name": "quiz-app-fullstack",
    "version": "1.0.0",
    "description": "Full-stack quiz application with React, Express, and MySQL",
    "main": "server/index.js",
    "scripts": {
        "start": "node server/index.js",
        "server": "nodemon server/index.js",
        "client": "cd client && npm start",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "install-client": "cd client && npm install",
        "build": "cd client && npm run build"
    },
    "dependencies": {
        "express": "^4.18.2",
        "mysql2": "^3.6.5",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express-validator": "^7.0.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "concurrently": "^8.2.2"
    },
    "keywords": [
        "quiz",
        "react",
        "express",
        "mysql",
        "fullstack"
    ],
    "author": "Your Name",
    "license": "MIT"
}